plugin.tx_pxadataprovider {
  settings {
    objectConfig {
      TYPO3\CMS\Extbase\DomainObject\AbstractEntity {
        key = entity
        includeProperties = uid,pid,_timestamp
        excludeProperties = pid
        remapProperties {
          uid = id
        }

        processProperties {
          _timestamp {
            data = date : c
          }
        }
      }
    }
  }
}
